{
  "name": "node-typescript-service",
  "version": "1.0.0",
  "description": "A RESTful API service built with Node.js, TypeScript, and Express.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon src/index.ts",
    "db:setup": "ts-node src/database/config/dbSetup.ts",
    "db:seed": "ts-node src/database/config/dbSeed.ts",
    "db:init": "ts-node src/database/config/dbInit.ts",
    "db:reset": "ts-node src/database/config/dbReset.ts && ts-node src/database/config/dbInit.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p .",
    "start": "node dist/index.js"
  },
  "author": "Simone Fermani",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.13",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/joi": "^17.2.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.0"
  }
}
